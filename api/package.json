    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "mongoose": "^8.3.2",
    "morgan": "^1.10.0",
    "pino": "^8.16.2",
    "prettier": "^3.2.5",
    "socket.io": "^4.7.5",
    "ws": "^8.17.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
